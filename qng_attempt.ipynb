      "source": [
        "import numpy as np\n",
        "import pennylane as qml\n",
        "from pennylane import expval, var"
      ],
      "execution_count": 0,
      "outputs": []
    },
    {
      "cell_type": "code",
      "metadata": {
        "id": "ngn7GRo7m6p4",
        "colab_type": "code",
        "colab": {
          "base_uri": "https://localhost:8080/",
          "height": 34
        },
        "outputId": "1529d1b4-17ee-4842-b687-d9ce29ce7b4a"
      },
      "source": [
        "dev = qml.device(\"default.qubit\", wires=1)\n",
        "\n",
        "@qml.qnode(dev)\n",
        "\n",
        "# create circuit, pauli-x and pauli-y operators\n",
        "def circuit(params):\n",
        "    qml.RX(params[0], wires=0)\n",
        "    qml.RY(params[1], wires=0)\n",
        "\n",
        "    # expectation value of pauli-z operator\n",
        "    return qml.expval(qml.PauliZ(0))\n",
        "\n",
        "# what are the appropriate numerical inputs??\n",
        "print(circuit([0.54, 0.12]))"
      ],
      "execution_count": 19,
      "outputs": [
        {
          "output_type": "stream",
          "text": [
            "0.8515405859048366\n"
          ],
          "name": "stdout"
        }
