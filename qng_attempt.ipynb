        "outputId": "4af92442-b205-4235-e8bc-c5c18c028f24"
      },
      "source": [
        "# calculuate gradient for quantum circuit function (the qnode) for parameters 0 and 1 \n",
        "@qml.qnode(dev)\n",
        "def circuit2(phi1, phi2):\n",
        "    qml.RX(phi1, wires=0)\n",
        "    qml.RY(phi2, wires=0)\n",
        "    return qml.expval(qml.PauliZ(0))\n",
        "\n",
        "dcircuit = qml.grad(circuit2, argnum=[0, 1])\n",
        "print(dcircuit(0.54, 0.12))"
      ],
      "execution_count": 22,
      "outputs": [
        {
          "output_type": "stream",
